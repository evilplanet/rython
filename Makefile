# metamake 1.1.0
# __init__.py 
# _bootstrap.py 
# _dependencies.py 
# _flags.py 
# _path.py 
# _shell.py 

BOOTSTRAPMETAMAKE=from base64 import b64decode;from imp import load_source;from tempfile import TemporaryFile, gettempdir;tempcodefile = TemporaryFile();tempcodefile.write(b64decode(''));tempcodefile.seek(0);makefile_metamake_bootstrapper = load_source('makefile_metamake_bootstrapper', gettempdir(), tempcodefile);makefile_metamake_bootstrapper.standalone_deserialize_metamake_from_makefile('Makefile')

__default:
	@python -c "$(BOOTSTRAPMETAMAKE);import sys;import os;sys.path.insert(0, os.getcwd());import Makefile;import metamake;metamake.run_default_task();"

ls:
	@python -c "$(BOOTSTRAPMETAMAKE);import sys;import os;sys.path.insert(0, os.getcwd());import Makefile;import metamake;metamake.list_tasks()"

%:
	@python -c "$(BOOTSTRAPMETAMAKE);import sys;import os;sys.path.insert(0, os.getcwd());import Makefile;import metamake;metamake.run_task('$@')"
